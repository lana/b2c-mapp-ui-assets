{
  "name": "@lana/b2c-mapp-ui-assets",
  "version": "5.8.0",
  "description": "Lana B2C Microapp UI Assets, icons and images for general use.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lana/b2c-mapp-ui-assets.git"
